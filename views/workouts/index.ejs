<%- include('../partials/header') %>
<table id="list">
    <thead>
      <tr>
        <th>Date</th>
        <th>Title</th>
        <th>Delete Workout</th>
      </tr>
    </thead>
    <tbody>
      <% workouts.forEach(function(w) { %>
        <tr>
          <td><%= w.name %></td>
          <!-- <td><%= w.releaseYear %></td> -->
          <td><a href="/workouts/<%= w._id %>">DETAILS</a></td>
          <td><form action = "/workouts/<%= w._id %>?_method=DELETE" method = "POST">
            <button type="submit">X</button>
          </form></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
<%- include('../partials/footer') %>